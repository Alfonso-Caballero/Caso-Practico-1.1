pipeline {
    agent none
    options {
        skipDefaultCheckout() // Evitar la clonaci√≥n predeterminada del repositorio
    }
    stages {
        stage('Get Code') {
            agent {
                label 'ubuntu'
            }
            steps {
                sh 'git clone https://github.com/Alfonso-Caballero/Caso-Practico-1.1.git'
                sh 'whoami'
                sh 'hostname'
                echo "${WORKSPACE}"
}
}
}
}
            
